{
  "name": "rtb-cli",
  "version": "1.0.8",
  "description": "基于 Rollup+TypeScript + Babel  搭建 ts脚手架",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "dev": "cross-env NODE_ENV=development rollup -c -w",
    "test": "jest",
    "plugins": "node ./plugins/index.js",
    "release": "npm run build && node ./plugins/deploy.js && node ./test/version.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tabxx/RTB-CLI.git"
  },
  "keywords": [
    "rollup",
    "typescript",
    "babel"
  ],
  "author": "Tab",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tabxx/RTB-CLI/issues"
  },
  "homepage": "https://github.com/Tabxx/RTB-CLI#readme",
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@types/jest": "^25.1.3",
    "cross-env": "^7.0.0",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^25.2.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "node-ssh": "^10.0.2",
    "node-wget": "^0.4.3",
    "shelljs": "^0.8.4",
    "zip-dir": "^1.0.2"
  }
}
